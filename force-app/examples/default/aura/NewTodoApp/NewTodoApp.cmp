<aura:component description="NewTodoApp" controller="TodoController">
    <ltng:require scripts="{!join(',',
                            $Resource.redux,
                            $Resource.reduxThunk)}"
                  afterScriptsLoaded="{!c.doInit}"/>
    <aura:attribute required="false" name="todoList" description="Todos" access="public" type="Todo__c[]" />
    <c:Redux aura:id="store" />
    <c:NewVisibilityFilter />
    <c:BrandNewTodo />
    <c:NewTodoToolbar />
    <c:NewTodoList todoList="{!v.todoList}"/>
    <c:NewTodoToolbar />
</aura:component>